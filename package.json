{
    "name": "zubairaziz.github.io",
    "author": "Zubair Aziz",
    "license": "MIT",
    "version": "1.0.0",
    "private": true,
    "dependencies": {
        "concurrently": "^4.1.0",
        "node-sass": "^4.12.0",
        "react": "^16.8.6",
        "react-animate-on-scroll": "^2.1.5",
        "react-dom": "^16.8.6",
        "react-router-dom": "^4.3.1",
        "react-scripts": "^3.0.0",
        "react-text-loop": "^2.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "sass": "node-sass -w src/scss/ -o src/css/ --recursive",
        "dev": "concurrently --kill-others \"react-scripts start\" \"node-sass -w src/scss/ -o src/css/ --recursive\"",
        "predeploy": "npm run build",
        "deploy": "gh-pages -b master -d build"
    },
    "devDependencies": {
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-universe": "^1.0.7",
        "eslint-plugin-import": "^2.17.2",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "gh-pages": "^2.0.1",
        "prettier": "^1.17.0",
        "prettier-eslint": "^8.8.2"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "homepage": "https://zubairaziz.github.io/"
